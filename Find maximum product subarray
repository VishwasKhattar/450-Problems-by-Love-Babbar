// Find maximum product subarray

#include <bits/stdc++.h>
using namespace std;

void MaxProd(int arr[], int n)
{
    int maxprod = 1;
    for (int i = 0; i < n; i++)
    {
        int prod = 1;
        for (int j = i; j < n; j++)
        {
            prod = prod * arr[j];
            maxprod = max(maxprod, prod);
        }
    }
    cout << "Maximum Subarray Product = " << maxprod;
}

int main()
{
    int arr[] = {6, -3, -10, 0, 2};
    int n = sizeof(arr) / sizeof(int);
    MaxProd(arr, n);
    return 0;
}
