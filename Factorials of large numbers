// Factorials of large numbers

#include <bits/stdc++.h>
using namespace std;

int factorial(unsigned long long int n)
{
    if (n == 0 || n == 1)
        return 1;
    else
        return n * factorial(n - 1);
}

int main()
{
    unsigned long long int n;
    cout << "Enter Number to find factorial = " << endl;
    cin >> n;
    cout << "Factorial = " << factorial(n);
    return 0;
}
